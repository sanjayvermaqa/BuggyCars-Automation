<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Westpac tests" verbose="0">
	<parameter name="browser" value="chrome" />

	<test name="User Registration Test" preserve-order="true">
		<classes>
			<class name="tests.RegisterUserTest" />
		</classes>
	</test>
	<test name="User Login Test">
		<classes>
			<class name="tests.LoginTest"></class>
		</classes>
	</test>
	<test name="Profile Test">
		<classes>
			<class name="tests.ProfileTest"></class>
		</classes>
	</test>
	<test name="Overall Page Test">
		<classes>
			<class name="tests.OverallPageTest"></class>
		</classes>
	</test>
	<test name="Voting Test">
		<classes>
			<class name="tests.VotingTest"></class>
		</classes>
	</test>

</suite>